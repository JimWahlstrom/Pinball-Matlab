function plot_animated(time, x, Vx, Ax, y, Vy, Ay)
       
regulator = 1000;


box_top_side = [-1 9, 7 7];
box_bottom_side = [-1 9, -12 -12];
box_left_side = [-1 -1, -12 7];
box_right_side = [9 9, -12 7];

figure
line()


for i =  1:length(time)/regulator
    plot(x(i*regulator), y(i*regulator),'o');
    axis([-2 9, -13 8]);
    pause(0.01);
    
end